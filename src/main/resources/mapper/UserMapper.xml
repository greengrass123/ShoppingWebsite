<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.netease.dao.mapper.UserMapper">

<insert id="save" parameterType="com.netease.entity.user">
        insert user(userName,password) values(#{userName},#{password})
</insert>
        <select id="getUsers" resultType="com.netease.entity.user">
                select * from user
        </select>
</mapper>